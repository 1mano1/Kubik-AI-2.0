{
    "models": {
        "flow": {
            "name": "KubikAI.models.cross_attention_flow.CrossAttentionFlowModel",
            "args": {
                "resolution": 16,
                "in_channels": 256,
                "model_channels": 384,
                "out_channels": 256,
                "cond_channels": 384,
                "image_feature_dim": 384,
                "num_blocks": 12,
                "num_head_channels": 64,
                "patch_size": 1,
                "use_fp16": false,
                "pe_mode": "ape"
            }
        }
    },
    "dataset": {
        "name": "KubikAI.datasets.latent_dataset.LatentImageDataset",
        "args": {
            "encoded_root": "/placeholder/path",
            "processed_root": "/placeholder/path",
            "resolution": 224
        }
    },
    "trainer": {
        "name": "KubikAI.trainers.flow_trainer.FlowTrainer",
        "args": {
            "max_steps": 1000000,
            "batch_size_per_gpu": 1,
            "optimizer": {
                "name": "AdamW",
                "args": {
                    "lr": 1e-5,
                    "weight_decay": 0.01
                }
            },
            "i_log": 1,
            "i_save": 5000,
            "t_schedule": {
                "name": "logitNormal",
                "args": { "mean": 0.0, "std": 1.0 }
            },
            "sigma_min": 1e-4,
            "p_uncond": 0.1,
            "image_cond_model": "dinov2_vits14_reg",
            "lambda_mse": 1.0
        }
    }
}
